@model IEnumerable<MovieListViewModel>

    <table>
        <tr>
            <th>Title</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title<a asp-controller="Movie" asp-action="Detail" asp-route-id="@item.Id">Details</a></td>
            </tr>
        }
    </table>
